"""

 *** *** 
*   *   *
*       *
 *     * 
  *   *  
   * *   
    *    

"""

for row in range(7):
    for col in range(9):
        if (row==0 and col%4!=0) or (row==1 and col%4==0) or (row-col==2) or (row+col==10):
            print("*",end="")
        else:
            print(end=" ")
    print()

